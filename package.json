{
  "name": "cryptic-dungeon-map-editor",
  "version": "1.0.0",
  "description": "A comprehensive map editor for the Cryptic Dungeon game engine",
  "main": "src/main.cpp",
  "scripts": {
    "build": "mkdir -p build && cd build && cmake .. && make",
    "clean": "rm -rf build",
    "run": "./build/MapEditor",
    "install-deps": "brew install glfw glm assimp imgui"
  },
  "dependencies": {
    "glfw": "^3.3.8",
    "glm": "^0.9.9.8",
    "assimp": "^5.2.5",
    "imgui": "^1.89.8",
    "glad": "^0.1.36"
  },
  "devDependencies": {
    "cmake": "^3.16.0",
    "make": "^4.3"
  },
  "keywords": [
    "game-engine",
    "map-editor",
    "3d-editor",
    "terrain-editor",
    "model-editor",
    "opengl",
    "c++"
  ],
  "author": "Cryptic Dungeon Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/CrypticDungeon.git"
  }
} 